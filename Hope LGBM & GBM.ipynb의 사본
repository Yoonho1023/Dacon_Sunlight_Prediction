target = 48 * 2\r\n","\r\n","### transform train\r\n","train_data, train_label = transform(dataset, label, 0,None, past_history,future_target, 1)\r\n","### transform test\r\n","test = []\r\n","for i in range(81):\r\n","    data = []\r\n","    tmp = pd.read_csv('/content/drive/MyDrive/Dacon/태양광 발전량 예측 AI 경진대회/test/' + str(i) + '.csv')\r\n","    tmp = tmp.loc[:, x_col].values\r\n","    tmp = tmp[-past_history:,:]\r\n","    data.append(np.ravel(tmp.T))\r\n","    data = np.array(data)\r\n","    test.append(data)\r\n","test = np.concatenate(test, axis=0)"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"G69vzz-YwzWt","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1610024036105,"user_tz":-540,"elapsed":5759,"user":{"displayName":"장윤호","photoUrl":"","userId":"16970831253759087565"}},"outputId":"9e604415-6cfb-4799-bd74-92297061c5de"},"source":["from sklearn import ensemble\r\n","N_ESTIMATORS = 1000\r\n","rf = ensemble.RandomForestRegressor(n_estimators=N_ESTIMATORS,\r\n","                                    max_features=1, random_state=0,\r\n","                                    max_depth = 5,\r\n","                                    verbose=True,\r\n","                                    n_jobs=-1)\r\n","rf.fit(train_data, train_label)\r\n","\r\n","rf_preds = []\r\n","for estimator in rf.estimators_:\r\n","    rf_preds.append(estimator.predict(test))\r\n","rf_preds = np.array(rf_preds)\r\n","\r\n","for i, q in enumerate(np.arange(0.1, 1, 0.1)):\r\n","    y_pred = np.percentile(rf_preds, q * 100, axis=0)\r\n","    submission.iloc[:, i] = np.ravel(y_pred)"],"execution_count":null,"outputs":[{"output_type":"stream","text":["[Parallel(n_jobs=-1)]: Using backend ThreadingBackend with 2 concurrent workers.\n","[Parallel(n_jobs=-1)]: Done  46 tasks      | elapsed:    0.2s\n","[Parallel(n_jobs=-1)]: Done 196 tasks      | elapsed:    0.6s\n","[Parallel(n_jobs=-1)]: Done 446 tasks      | elap