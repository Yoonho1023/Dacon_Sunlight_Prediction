                         learning_rate=.01, min_samples_leaf=20,\r\n","                                min_samples_split=20)\r\n","   mod.fit(X_train_1, Y_train_1)\r\n","\r\n","   # (b) Predictions\r\n","   pred = pd.Series(mod.predict(X_test).round(2))\r\n","   return pred, mod\r\n","\r\n","def train_data(X_train, Y_train, X_valid, Y_valid, X_test):\r\n","\r\n","  GBM_models=[]\r\n","  GBM_actual_pred = pd.DataFrame()\r\n","\r\n","  for q in quantiles:\r\n","      pred , model = GBM(q, X_train, Y_train, X_valid, Y_valid, X_test)\r\n","      GBM_models.append(model)\r\n","      GBM_actual_pred = pd.concat([GBM_actual_pred,pred],axis=1)\r\n","  GBM_actual_pred.columns = quantiles\r\n","\r\n","  return GBM_models, GBM_actual_pred\r\n"],"execution_count":null,"outputs":[]},{"cell_type":"code","metadata":{"id":"RzZruc5rOg25"},"source":[""],"execution_count":null,"outputs":[]},{"cell_type":"markdown","metadata":{"id":"bgNJEd-FpyZs"},"source":["## 수정된 baseline\r\n"]},{"cell_type":"code","metadata":{"id":"moF_clyUOg0R","executionInfo":{"status":"ok","timestamp":1610272355153,"user_tz":-540,"elapsed":2399,"user":{"displayName":"장윤호","photoUrl":"","userId":"16970831253759087565"}}},"source":["import pandas as pd\r\n","import numpy as np\r\n","import os\r\n","import glob\r\n","import random\r\n","\r\n","import warnings\r\n","warnings.filterwarnings(\"ignore\")"],"execution_count":1,"outputs":[]},{"cell_type":"code","metadata":{"id":"BuqBvGovOgxT","colab":{"base_uri":"https://localhost:8080/"},"executionInfo":{"status":"ok","timestamp":1610272380369,"user_tz":-540,"elapsed":23907,"user":{"displayName":"장윤호","photoUrl":"","userId":"16970831253759087565"}},"outputId":"c453acff-2df2-4462-c636-e348415cce90"},"source":["from google.colab import drive\r\n","drive.mount('/content/drive')"],"execution_count":2,"outputs":[{"output_type":"stream","text":["Mounted at /content/drive\n"],"name":"stdout"}]},{"cell_type":"code","metadata":{"id":"XWuT8xGMOgua","executionInfo":{"status":"ok","